  let elem = document.getElementById('youtube-{{ include.id }}');
  elem.innerHTML = '<iframe width="500" height="281" src="https://www.youtube.com/embed/{{ include.id }}" frameborder="0" allowfullscreen></iframe>'
}
</script>
